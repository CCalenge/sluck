{
  "name": "sluck",
  "version": "0.1.0",
  "description": "Du Slack avec un U",
  "main": "app.js",
  "dependencies": {
    "client-sessions": "^0.7.0",
    "body-parser": "^1.15.2",
    "ejs": "^2.5.1",
    "ent": "^2.2.0",
    "express": "^4.14.0",
    "mysql": "^2.11.1",
    "socket.io": "^1.4.8",
    "nodemon":"^1.10.2"
  },
  "devDependencies": {},
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "git": "git pull",
    "buildSluckJS": "browserify public/js/sluck/main.js | uglifyjs > public/js/sluck.js",
    "buildCSS": "sass sass/sluck.scss:public/css/sluck.css --style compressed",
    "watchJS": "watchify public/js/sluck/main.js -o public/js/sluck.js",
    "watchLoginSass": "sass --watch sass/login.scss:public/css/login.css --style compressed",
    "watchSluckSass": "sass --watch sass/sluck.scss:public/css/sluck.css --style compressed",
    "watchSASS": "npm run watchLoginSass & npm run watchSluckSass",
    "buildJS": "npm run buildSluckJS",
    "dev": "npm run watchJS & npm run watchSASS & nodemon --config nodemon.json app.js",
    "devWin": "npm run buildJS && npm run buildCSS && nodemon --config nodemonWin.json",
    "server": "npm run git && npm run buildJS && node app.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Maximums/sluck.git"
  },
  "author": "maxfqs maxdef",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Maximums/sluck/issues"
  },
  "homepage": "https://github.com/Maximums/sluck#readme"
}
